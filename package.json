{
  "name": "box-webpack",
  "version": "0.1.0",
  "author": "Konrad Abe",
  "license": "MIT",
  "description": "A small clean slate project to use as a base setup with reacth, webpack, storybooks and more",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KADlancer/boxWebpack.git"
  },
  "bugs": {
    "url": "https://github.com/KADlancer/boxWebpack/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --config webpack.development.config.js",
    "build": "webpack --config webpack.development.config.js",
    "buildProd": "webpack --config webpack.production.config.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test:lint:js": "eslint src --ext .jsx,.js",
    "test:lint:css": "stylelint --config=.stylelintrc '**/*.scss'",
    "test:lint": "run-s test:lint:**",
    "fix:js": "eslint --fix src --ext .jsx,.js",
    "fix:css": "stylelint --fix --config=.stylelintrc '**/*.scss'",
    "fix": "run-s fix:**"
  },
  "homepage": "https://github.com/KADlancer/boxWebpack#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^4.0.0-alpha.20",
    "@storybook/addon-actions": "^4.0.0-alpha.20",
    "@storybook/addon-info": "^4.0.0-alpha.20",
    "@storybook/addon-knobs": "^4.0.0-alpha.20",
    "@storybook/addon-notes": "^4.0.0-alpha.20",
    "@storybook/addon-options": "^4.0.0-alpha.20",
    "@storybook/addon-viewport": "^4.0.0-alpha.20",
    "@storybook/addons": "^4.0.0-alpha.20",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "mini-css-extract-plugin": "^0.4.2",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-nested": "^2.1.2",
    "prettier": "^1.14.2",
    "sass-loader": "^7.1.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@storybook/react": "^4.0.0-alpha.20",
    "carbon-components": "^9.10.0",
    "carbon-components-react": "^6.28.1",
    "carbon-icons": "^7.0.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.3",
    "why-did-you-update": "^0.1.1"
  }
}
